{
    "name": "chandraanwar91/elasticsearch",
    "description": "Laravel, Lumen and Native php elasticseach query builder to build complex queries using an elegant syntax",
    "keywords": [
        "elasticsearch",
        "php",
        "laravel",
        "lumen",
        "scout",
        "fulltext",
        "indexing",
        "builder"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "chandraanwar",
            "email": "chandraanwar91@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chandraanwar91\\Elasticsearch\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.6.6",
        "elasticsearch/elasticsearch": "^5.0",
        "illuminate/pagination": "*",
        "illuminate/support": "*",
        "symfony/var-dumper": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chandraanwar91\\Elasticsearch\\ElasticsearchServiceProvider"
            ],
            "aliases": {
                "ES": "Chandraanwar91\\Elasticsearch\\Facades\\ES"
            }
        }
    }
}
